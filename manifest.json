{

    "manifest_version": 2,
    "name": "Podcast France Culture Téléchargement",
    "version": "1.0",
  
    "description": "Téléchargement de podcasts France culture sur toutes les pages web correspondant à franceculture.fr",
  
    "icons": {
      "48": "icons/icon-fc.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.franceculture.fr/*"],
        "js": ["downloadPodcast.js"],
        "css": ["stylesDL.css"]
      }
    ],

    "web_accessible_resources": ["icons/Cloud-Download-256.png"]
  
  }